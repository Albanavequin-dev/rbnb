<div class="container py-5">
  <div class="row">
    <div class="col-12 col-md-6 offset-lg-3">
      <div class="bg-white rounded p-3 shadow">
      <% if @my_rental.nil? %>
        <h3>Modify my booking</h1>
          <%= render "customer-form-booking", task: @booking %>
      <% else %>
      <h3>Accept or refuse the booking demand</h3>
          <%= render "owner-form-booking", task: @booking %>
      <% end %>
      </div>
    </div>
  </div>
</div>
